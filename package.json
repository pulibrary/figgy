    "lint": "yarn run eslint -- --ext .vue app/javascript"
  },
  "dependencies": {
    "@babel/preset-react": "^7.8.3",
    "@google-cloud/pubsub": "^0.31.0",
    "@google-cloud/storage": "^5.8.4",
    "@rails/webpacker": "https://github.com/rails/webpacker#v5.4.3",
    "ajv": "^6.12.3",
    "apollo-boost": "^0.1.10",
    "axios": "^0.21.2",
    "core-js": "^3.16.2",
    "flush-promises": "^1.0.2",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.9.2",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.21",
    "lux-design-system": "4.0.2",
    "mapnik": "^4.5.9",
    "npm": "^8.5.5",
    "openseadragon": "^2.3.1",
    "path": "^0.12.7",
    "postcss-smart-import": "^0.7.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux-logger": "^3.0.6",
    "unfetch": "^4.0.1",
    "vue": "^2.5.17",
    "vue-details": "^1.1.0",
    "vue-loader": "^15.4.2",
    "vue-select": "^3.4.0",
    "vue-template-compiler": "^2.5.17",
    "vue2-dropzone": "^3.6.0",
    "vuex": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.2.5",
    "eslint": "^5.11.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.1.0",
    "jest": "^23.6.0",
    "jest-canvas-mock": "^1.1.0",
    "jest-serializer-vue": "^2.0.2",
    "jquery": "^3.5.0",
    "vue-jest": "^3.0.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "dotenv": "^7.0.0",
    "identity-obj-proxy": "^3.0.0"
  },
  "jest": {
    "roots": [
      "app/javascript/test",
      "cloud_fixity/test"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/javascript",
      "app/assets/javascripts"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2|svg)$": "identity-obj-proxy",
      "^@/(.*)$": "app/javascript/$1"
    },
    "moduleFileExtensions": [
      "es6",
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.es6$": "<rootDir>/node_modules/babel-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupTestFrameworkScriptFile": "./app/javascript/test/setup.js"
  }
}
