<%= simple_form_for(@change_set) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :drive_barcode %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
    <%= link_to "Cancel", solr_document_path(f.object.id), class: 'btn btn-secondary' %>
  </div>
<% end %>
