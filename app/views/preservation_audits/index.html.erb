<p style="color: green"><%= notice %></p>

<% content_for :title, "Preservation audits" %>

<h1>Preservation audits</h1>

<div id="preservation_audits">
  <% @preservation_audits.each do |preservation_audit| %>
    <h2>
      <%= link_to "Audit #{preservation_audit.id}", preservation_audit %>
    </h2>
    <%= render preservation_audit %>
  <% end %>
</div>
