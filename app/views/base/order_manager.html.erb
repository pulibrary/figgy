<div class="container">
<ul class="breadcrumb">
  <% if decorated_change_set_resource.try(:decorated_parent).present? %>
    <li><%= link_to decorated_change_set_resource.decorated_parent.first_title, solr_document_path(id: "#{decorated_change_set_resource.decorated_parent.id}") %></li>
  <% end %>
  <li><%= link_to decorated_change_set_resource.first_title, solr_document_path(id: "#{@change_set.id}") %></li>
  <li class="active">Order Manager</li>
</ul>
</div>
<order-manager resource-id="<%= @change_set.id %>" default-thumbnail="<%= image_url('default_thumbnail.png') %>"></order-manager>
