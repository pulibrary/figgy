{
  "presets": [
    ["env", {
      "presets": [["es2015", { "modules": false }]],
      "transformIgnorePatterns": ["<rootDir>/node_modules/(?!vue)" ],
      "targets": {
        "browsers": "> 1%",
        "uglify": true
      },
      "useBuiltIns": true
    }]
  ],

  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    ["transform-class-properties", { "spec": true }]
  ],

}
